    "# ***********\n",
    "# Question 1\n",
    "# ***********\n",
    "# Find the maximum number in the list without using max().\n",
    "\n",
    "numbers = [3, 5, 2, 8, 1, 4]\n",
    "\n",
    "max_number = numbers[0]\n",
    "for num in numbers:\n",
    "    if num > max_number:\n",
    "        max_number = num\n",
    "    \n",
    "print(\"The maximum number is:\", max_number)"
